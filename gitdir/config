[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "si"]
	url = git@github.com:studentingegneria/.
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "dev"]
	remote = si
	merge = refs/heads/release
[branch "release"]
	remote = si
	merge = refs/heads/release
[gitflow "branch"]
	master = release
	develop = dev
[gitflow "prefix"]
	feature = feature/
	bugfix = bugfix/
	release = rc/
	hotfix = hotfix/
	support = support/
	versiontag = v
[gitflow "path"]
	hooks = .git/hooks
